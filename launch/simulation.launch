	<node pkg="Simulator" type="cell_node" name="cell0" args="0 0 0 1" output="screen"/>
	<node pkg="Simulator" type="cell_node" name="cell1" args="1 -1 1 1" output="screen"/>
	<node pkg="Simulator" type="cell_node" name="cell2" args="2 1 1 1" output="screen"/>
	<node pkg="Simulator" type="cell_node" name="cell3" args="3 -1 2 1" output="screen"/>
	<node pkg="Simulator" type="cell_node" name="cell4" args="4 1 2 1" output="screen"/>
	<node pkg="Simulator" type="cell_node" name="cell5" args="5 -1 3 1" output="screen"/>
	<node pkg="Simulator" type="cell_node" name="cell6" args="6 1 3 1" output="screen"/>
</launch>